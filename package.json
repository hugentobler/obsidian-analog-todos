{
	"name": "obsidian-analog-todos",
	"version": "1.0.0",
	"description": "Minimal todos and tasks inspired by Ugmonk Analog paper productivity system.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
