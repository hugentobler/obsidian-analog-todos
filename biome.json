{
	"$schema": "https://biomejs.dev/schemas/2.3.8/schema.json",
	"assist": {
		"actions": {
			"source": {
				"organizeImports": "on"
			}
		}
	},
	"linter": {
		"rules": {
			"suspicious": {
				"noPrototypeBuiltins": "off",
				"noEmptyBlockStatements": "off"
			}
		}
	},
	"files": {
		"includes": ["**", "!**/node_modules", "!**/main.js"]
	}
}
